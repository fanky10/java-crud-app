<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>ar.edu.utn.frro.java.web.cwa.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>home</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>BicicletasServlet</display-name>
    <servlet-name>BicicletasServlet</servlet-name>
    <servlet-class>ar.edu.utn.frro.java.web.cwa.BicicletasServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BicicletasServlet</servlet-name>
    <url-pattern>/bicicletas</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>UserFilter</display-name>
    <filter-name>UserFilter</filter-name>
    <filter-class>ar.edu.utn.frro.java.web.cwa.UserFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>UserFilter</filter-name>
    <url-pattern>/secure/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>SecuredResourcesServlet</display-name>
    <servlet-name>SecuredResourcesServlet</servlet-name>
    <servlet-class>ar.edu.utn.frro.java.web.cwa.SecuredResourcesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SecuredResourcesServlet</servlet-name>
    <url-pattern>/secure/usuario</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
